    <GradientStopCollection x:Key="GradientFill1">
        <GradientStop Color="{StaticResource CreditBackColor1}" Offset="0"/>
        <GradientStop Color="{StaticResource CreditBackColor2}" Offset="0.5"/>
    </GradientStopCollection>

    <GradientStopCollection x:Key="GradientFill2">
        <GradientStop Color="{StaticResource doughnutColor3}" Offset="0"/>
        <GradientStop Color="{StaticResource doughnutColor4}" Offset="0.5"/>
    </GradientStopCollection>

    <GradientStopCollection x:Key="GradientFill3">
        <GradientStop Color="{StaticResource doughnutColor5}" Offset="0"/>
        <GradientStop Color="{StaticResource doughnutColor6}" Offset="0.5"/>
    </GradientStopCollection>

    <GradientStopCollection x:Key="GradientFill4">
        <GradientStop Color="{StaticResource doughnutColor7}" Offset="0.0"/>
        <GradientStop Color="{StaticResource doughnutColor8}" Offset="0.25"/>
    </GradientStopCollection>

    <GradientStopCollection x:Key="TransparentFill">
        <GradientStop Color="Transparent" Offset="0.0"/>
        <GradientStop Color="Transparent" Offset="0.25"/>
    </GradientStopCollection>

    <SolidColorBrush x:Key="dataGridBorderBrush">#282828</SolidColorBrush>

